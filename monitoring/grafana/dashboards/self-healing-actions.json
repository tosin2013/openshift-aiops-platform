{
  "dashboard": {
    "id": null,
    "title": "Self-Healing Actions",
    "tags": ["self-healing", "coordination", "aiops", "actions"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Coordination Engine Status",
        "type": "stat",
        "targets": [
          {
            "expr": "coordination_engine_status",
            "legendFormat": "Engine Status"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            },
            "mappings": [
              {"options": {"0": {"text": "UNHEALTHY"}}, "type": "value"},
              {"options": {"1": {"text": "HEALTHY"}}, "type": "value"}
            ]
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Active Actions",
        "type": "stat",
        "targets": [
          {
            "expr": "coordination_engine_active_actions",
            "legendFormat": "Active Actions"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 50}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Actions by Type",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (action_type) (rate(coordination_engine_actions_total[5m]))",
            "legendFormat": "{{action_type}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Action Processing Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(coordination_engine_actions_total[5m])",
            "legendFormat": "{{action_type}} - {{source}}"
          }
        ],
        "yAxes": [
          {"label": "Actions/sec", "min": 0}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 5,
        "title": "Conflict Detection Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(coordination_engine_conflicts_total[5m])",
            "legendFormat": "{{conflict_type}}"
          }
        ],
        "yAxes": [
          {"label": "Conflicts/sec", "min": 0}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 6,
        "title": "Resolution Time",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(coordination_engine_resolution_seconds_bucket[5m]))",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, rate(coordination_engine_resolution_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.99, rate(coordination_engine_resolution_seconds_bucket[5m]))",
            "legendFormat": "99th percentile"
          }
        ],
        "yAxes": [
          {"label": "Resolution Time (seconds)", "min": 0}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 7,
        "title": "Workflow Execution Status",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(mcp_server_workflow_executions_total[5m])",
            "legendFormat": "{{workflow_type}} - {{status}}"
          }
        ],
        "yAxes": [
          {"label": "Workflows/sec", "min": 0}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 8,
        "title": "Anomaly Detection Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(mcp_server_anomaly_detections_total[5m])",
            "legendFormat": "{{severity}} - {{model_type}}"
          }
        ],
        "yAxes": [
          {"label": "Anomalies/sec", "min": 0}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 9,
        "title": "Recent Actions Timeline",
        "type": "table",
        "targets": [
          {
            "expr": "coordination_engine_actions_total",
            "format": "table",
            "instant": true
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {},
              "renameByName": {
                "action_type": "Action Type",
                "source": "Source",
                "Value": "Count"
              }
            }
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      },
      {
        "id": 10,
        "title": "Health Check Status",
        "type": "graph",
        "targets": [
          {
            "expr": "mcp_server_health_status",
            "legendFormat": "{{component}}"
          }
        ],
        "yAxes": [
          {"label": "Health Status", "min": 0, "max": 1}
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 32}
      }
    ]
  }
}
